{
  "name": "packages/cqflow-evaluator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cqflow-evaluator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/cqflow-evaluator",
        "main": "packages/cqflow-evaluator/src/index.ts",
        "tsConfig": "packages/cqflow-evaluator/tsconfig.lib.json",
        "assets": []
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs packages/cqflow-evaluator {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/cqflow-evaluator/**/*.ts",
          "packages/cqflow-evaluator/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/cqflow-evaluator"
      }
    },
    "run-thing": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/cqflow-evaluator",
        "command": "npx ts-node --project tsconfig.json -r tsconfig-paths/register ./src/index.ts"
      }
    }
  },
  "tags": []
}
